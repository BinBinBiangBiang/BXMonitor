{
  "name": "bx-monitor",
  "version": "1.0.0",
  "description": "A performance monitoring SDK for web applications",
  "keywords": ["performance", "monitor", "web", "analytics"],
  "author": "BiangBiang",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/bx-monitor"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "type-check": "tsc --noEmit",
